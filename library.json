{
    "name": "esp-tflite-micro",
    "version": "1.3.3.1",
    "description": "TensorFlow Lite Micro for Espressif Chipsets",
    "keywords": "tflite",
    "repository": {
        "type": "git",
        "url": "https://github.com/espressif/esp-tflite-micro.git"
    },
    "export": {
        "include": [
            "tensorflow/*",
            "third_party/*",
            "library.json",
            "README.md",
            "LICENSE",
            "signal/*",
            "scripts",
            "examples"
        ]
    },
    "license": "Apache-2.0",
    "build": {
        "includeDir": ".",
        "srcDir": ".",
        "flags": [
            "-I third_party/gemmlowp",
            "-I third_party/flatbuffers/include",
            "-I third_party/ruy",
            "-I third_party/kissfft",
            "-D ESP_NN",
            "-D TF_LITE_STATIC_MEMORY",
            "-D DTF_LITE_DISABLE_X86_NEON",
            "-O3"
        ],
        "srcFilter": [
            "-<*>",
            "+<tensorflow/lite/debug_log.cc>",
            "+<tensorflow/lite/flatbuffer_utils.cc>",
            "+<tensorflow/lite/memory_helpers.cc>",
            "+<tensorflow/lite/micro_allocation_info.cc>",
            "+<tensorflow/lite/micro_allocator.cc>",
            "+<tensorflow/lite/micro_context.cc>",
            "+<tensorflow/lite/micro_interpreter_context.cc>",
            "+<tensorflow/lite/micro_interpreter_graph.cc>",
            "+<tensorflow/lite/micro_interpreter.cc>",
            "+<tensorflow/lite/micro_log.cc>",
            "+<tensorflow/lite/micro_op_resolver.cc>",
            "+<tensorflow/lite/micro_profiler.cc>",
            "+<tensorflow/lite/micro_resource_variable.cc>",
            "+<tensorflow/lite/esp/micro_time.cc>",
            "+<tensorflow/lite/micro_utils.cc>",
            "+<tensorflow/lite/recording_micro_allocator.cc>",
            "+<tensorflow/lite/system_setup.cc>",
			"+<tensorflow/lite/kernels/*.c>",
			"+<tensorflow/lite/kernels/*.cc>",
            "-<tensorflow/lite/kernels/add.cc>",
            "-<tensorflow/lite/kernels/conv.cc>",
            "-<tensorflow/lite/kernels/depthwise_conv.cc>",
            "-<tensorflow/lite/kernels/fully_connected.cc>",
            "-<tensorflow/lite/kernels/mul.cc>",
            "-<tensorflow/lite/kernels/pooling.cc>",
            "-<tensorflow/lite/kernels/softmax.cc>",
            "+<tensorflow/lite/tflite_bridge/*.c>",
            "+<tensorflow/lite/tflite_bridge/*.cc>",
            "+<tensorflow/lite/experimental/microfrontend/lib/*.c>",
            "+<tensorflow/lite/experimental/microfrontend/lib/*.cc>",
            "+<signal/micro/kernels/*.c>",
            "+<signal/micro/kernels/*.cc>",
			"+<signal/src/*.c>",
			"+<signal/src/*.cc>",
			"+<signal/src/kiss_fft_wrappers/kiss_fft_float.cc>",
			"+<signal/src/kiss_fft_wrappers/kiss_fft_int16.cc>",
			"+<signal/src/kiss_fft_wrappers/kiss_fft_int32.cc>",
			"+</tensorflow/lite/kernels/esp_nn/*.cc>",
			"+</tensorflow/lite/kernels/kernel_util.cc>",
			"+</tensorflow/lite/micro/memory_planner/greedy_memory_planner.cc>",
			"+</tensorflow/lite/micro/memory_planner/linear_memory_planner.cc>",
			"+</tensorflow/lite/micro/arena_allocator/non_persistent_arena_buffer_allocator.cc>",
			"+</tensorflow/lite/micro/arena_allocator/persistent_arena_buffer_allocator.cc>",
			"+</tensorflow/lite/micro/arena_allocator/recording_single_arena_buffer_allocator.cc>",
			"+</tensorflow/lite/micro/arena_allocator/single_arena_buffer_allocator.cc>",
			"+</tensorflow/lite/core/c/common.cc>",
			"+</tensorflow/lite/core/api/flatbuffer_conversions.cc>",
			"+</tensorflow/lite/core/api/tensor_utils.cc>",
			"+</tensorflow/lite/kernels/internal/common.cc>",
			"+</tensorflow/lite/kernels/internal/quantization_util.cc>",
			"+</tensorflow/lite/kernels/internal/portable_tensor_utils.cc>",
			"+</tensorflow/lite/kernels/internal/tensor_utils.cc>",
			"+</tensorflow/lite/kernels/internal/tensor_ctypes.cc>",
			"+</tensorflow/lite/kernels/internal/reference/portable_tensor_utils.cc>",
			"+</tensorflow/lite/kernels/internal/reference/comparisons.cc>",
			"+</tensorflow/compiler/mlir/lite/core/api/error_reporter.cc>",
			"+</tensorflow/compiler/mlir/lite/schema/schema_utils.cc>"
        ]
    }
}
